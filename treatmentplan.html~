     	<script>
var i=0;

function plus(){
var element=document.createElement("div");
element.innerHTML="<div class='container' id="+i+"><div class='jumbotron'><form class='form-horizontal' role='form'><div class='form-group'><label class='control-label col-xs-4' for='treatment_name'>Treatment Name</label><div class='col-xs-8'><input type='treatment_name' class='form-control' id='treatment_name'></div></div><div class='form-group'><label class='control-label col-xs-5' for='trail_code'>Trail Code (if applicable):</label><div class='col-xs-7'><input type='trail_code' class='form-control' id='trail_code'></div></div><div class='form-group'><label class='control-label col-xs-3' for='start_date'>Start Date:</label><div class='col-xs-9'><input type='start_date' class='form-control' id='start_date'></div></div><div class='form-group'><label class='control-label col-xs-5' for='frequency'>How frequently is it given?:</label><div class='col-xs-7'><input type='frequency' class='form-control' id='frenquency'></div></div><div class='form-group'><label class='control-label col-xs-5' for='propose'>Proposed Number of treatments:</label><div class='col-xs-7'><input type='propose' class='form-control' id='propose'></div></div></form><button type='button' class='btn btn-default glyphicon glyphicon-minus pull-right' onClick='minus("+i+")'></button></div></div>";
var form = document.getElementById("input");
form.appendChild(element);
i++;
}


function minus(){
var item=document.getElementById(arguments[0]);
item.parentNode.removeChild(item);
}

</script>
